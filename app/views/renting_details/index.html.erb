<h1>Listing renting_details</h1>

<table>
  <thead>
    <tr>
      <th>Shortdescription</th>
      <th>Longdescription</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @renting_details.each do |renting_detail| %>
      <tr>
        <td><%= renting_detail.shortdescription %></td>
        <td><%= renting_detail.longdescription %></td>
        <td><%= link_to 'Show', renting_detail %></td>
        <td><%= link_to 'Edit', edit_renting_detail_path(renting_detail) %></td>
        <td><%= link_to 'Destroy', renting_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Renting detail', new_renting_detail_path %>
